.auto-grid
  // No Grid spoupport
  display flex
  flex-wrap wrap

  @supports (display: grid)
    display grid
    grid-template-columns repeat(auto-fill, minmax(200px, 1fr))
    grid-auto-rows minmax(150px, auto)
    grid-gap 10px // @stylint ignore // Too new for stylint 1.5.9


.auto-grid__item
  height 100%

  // No Grid support
  flex 1 1 200px
  min-height 150px
  margin 5px

  @supports (display: grid)
    margin 0
