{
  "name": "broadcast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:styles": "stylus -m -c -u stylus-mixins -u responsive-grid -u poststylus --with \"['autoprefixer']\" ./src/asset/stylus/index*.styl -o ./src/asset/css/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:js": "eslint . --cache",
    "test": "react-scripts test --env=jsdom",
    "watch:styles": "npm run build:styles -- -w",
    "watch": "concurrently --names \"webpack,stylus\" --prefix name \"npm run start\" \"npm run watch:styles\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "eject": "react-scripts eject",
    "fix:js": "npm run js:lint -- --fix"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.4.0",
    "deep-freeze": "^0.0.1",
    "enzyme": "^2.8.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "poststylus": "^0.2.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-snapshot": "^1.1.0",
    "react-test-renderer": "^15.5.4",
    "responsive-grid": "^1.1.0",
    "sinon": "^2.3.4",
    "stylus": "^0.54.5",
    "stylus-mixins": "^0.4.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.7"
  }
}
